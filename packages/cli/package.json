{
  "name": "webhook-proxy-cli",
  "version": "1.0.1",
  "description": "CLI tool for managing webhook-proxy proxies with GitHub OAuth authentication",
  "type": "module",
  "bin": {
    "webhook-proxy": "./dist/index.js",
    "whp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build",
    "postinstall": "chmod +x ./dist/index.js || true"
  },
  "keywords": [
    "webhook",
    "proxy",
    "cli",
    "github",
    "oauth",
    "webhook-proxy",
    "developer-tools"
  ],
  "author": "lc-cn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lc-cn/webhook-proxy.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/lc-cn/webhook-proxy/tree/master/packages/cli#readme",
  "bugs": {
    "url": "https://github.com/lc-cn/webhook-proxy/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "CLI_INTEGRATION.md"
  ],
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "open": "^10.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

